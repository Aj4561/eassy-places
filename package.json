{
  "name": "easy-places",
  "version": "1.0.0",
  "description": "Lightweight country-state-city data manager for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [
    "country",
    "state",
    "city",
    "location",
    "region",
    "places",
    "npm",
    "geodata",
    "express",
    "sequelize",
    "postgresql",
    "api"
  ],
  "author": "Ajay Kaithwas <ajkaithwas4561@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aj4561/eassy-places.git"
  },
  "bugs": {
    "url": "https://github.com/Aj4561/eassy-places/issues"
  },
  "homepage": "https://github.com/Aj4561/eassy-places#readme",
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.3"
  }
}
